# PrismIQ Backend Environment Variables
# Copy this file to .env and fill in the values

# Application
APP_NAME=PrismIQ
APP_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# Server
HOST=0.0.0.0
PORT=8000
RELOAD=true

# CORS (comma-separated origins)
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# OpenAI (for LangChain agent)
OPENAI_API_KEY=sk-your-api-key-here

# Database (future use)
# DATABASE_URL=postgresql://user:password@localhost:5432/prismiq
